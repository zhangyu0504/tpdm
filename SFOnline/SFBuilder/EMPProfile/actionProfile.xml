		<Element id="DecryptDataAction" name="交易数据解密" implClass="com.ecc.emp.action.DecryptDataAction" wrapperClass="com.ecc.ide.editor.visualflow.ActionElementWrapper" iconName="/images/newElement.gif">
			<attributes>
				<attr id="states" editable="false" valueList="0=操作成功;1=异常;Other=其他;">
						</attr>
