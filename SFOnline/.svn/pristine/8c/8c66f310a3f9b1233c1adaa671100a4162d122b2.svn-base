<?xml version="1.0" encoding="GBK" ?>


<web-app version="2.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns="http://java.sun.com/xml/ns/javaee">
	<!--  JMX Manager setting param load by startuplistener -->
	<!--  EMPLog setting param load by startuplistener -->
	<!--  EMP Startup Listener to startup EMP -->
	<context-param>
		<param-name>logImplClass</param-name>
		<param-value>com.ecc.emp.log.EMPLog4jLog</param-value>
	</context-param>
	<context-param>
		<param-name>resourceFileName</param-name>
		<param-value>WEB-INF/commons/resource.xml</param-value>
	</context-param>
	<context-param>
		<param-name>logFileName</param-name>
		<param-value>WEB-INF/empLog.log</param-value>
	</context-param>
	<context-param>
		<param-name>logSettingFile</param-name>
		<param-value>WEB-INF/commons/logging.xml</param-value>
	</context-param>
	<!--<context-param>
		<param-name>JMXContextFile</param-name>
		<param-value>WEB-INF/EMPJMXContext.xml</param-value>
	</context-param>
	--><context-param>
		<param-name>functionFileName</param-name>
		<param-value>WEB-INF/commons/function.xml</param-value>
	</context-param>
	<filter>
		<filter-name>SFBaseEncoding</filter-name>
		<filter-class>com.ecc.emp.web.jsptags.SetCharacterEncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>SFBaseEncoding</filter-name>
		<url-pattern>*.do</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>core.emp.servlet.SFStartupListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>SFOnlineServlet</servlet-name>
		<servlet-class>core.emp.servlet.EMPRequestServlet</servlet-class>
		<init-param>
			<param-name>iniFile</param-name>
			<param-value>WEB-INF/bizs/SFOnline/settings.xml</param-value>
		</init-param>
		<init-param>
			<param-name>enableInitialize</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<param-name>password</param-name>
			<param-value>eccemp</param-value>
		</init-param>
		<init-param>
			<param-name>factoryName</param-name>
			<param-value>SFOnline</param-value>
		</init-param>
		<init-param>
			<param-name>rootContextName</param-name>
			<param-value>rootCtx</param-value>
		</init-param>
		<init-param>
			<param-name>servletContextFile</param-name>
			<param-value>WEB-INF/mvcs/SFOnline/empServletContext.xml</param-value>
		</init-param>
		<init-param>
			<param-name>jspRootPath</param-name>
			<param-value>WEB-INF/mvcs/SFOnline/</param-value>
		</init-param>
		<init-param>
			<param-name>resourceFileName</param-name>
			<param-value>/WEB-INF/commons/resource.xml</param-value>
		</init-param>
		<init-param>
			<param-name>contentDivId</param-name>
			<param-value>area_content</param-value>
		</init-param>
		<init-param>
			<param-name>logType</param-name>
			<param-value>log4j</param-value>
		</init-param>
		<init-param>
			<param-name>logSettingFile</param-name>
			<param-value>WEB-INF/commons/logging.xml</param-value>
		</init-param>
		<init-param>
			<param-name>logFileName</param-name>
			<param-value>WEB-INF/empLog.log</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<!-- ESB通信-->
	<servlet>
		<servlet-name>ESBChannel</servlet-name>
		<servlet-class>core.communication.access.esb.PBankTCPIPServiceServlet</servlet-class>
		<init-param>
			<param-name>factoryName</param-name>
			<param-value>SFOnline</param-value>
		</init-param>
		<init-param>
			<param-name>rootContextName</param-name>
			<param-value>rootCtx</param-value>
		</init-param>
		<init-param>
			<param-name>servletContextFile</param-name>
			<param-value>WEB-INF/channels/ESBChannel.xml</param-value>
		</init-param>
		<init-param>
			<param-name>sessionIdField</param-name>
			<param-value/>
		</init-param>	
		<load-on-startup>2</load-on-startup>
	</servlet>
	<!-- 合作行通信-->
	<servlet>
		<servlet-name>CobankChannel</servlet-name>
		<servlet-class>core.communication.access.tcpip.PBankTCPIPServiceServlet</servlet-class>
		<init-param>
			<param-name>factoryName</param-name>
			<param-value>SFOnline</param-value>
		</init-param>
		<init-param>
			<param-name>rootContextName</param-name>
			<param-value>rootCtx</param-value>
		</init-param>
		<init-param>
			<param-name>servletContextFile</param-name>
			<param-value>WEB-INF/channels/CobankChannel.xml</param-value>
		</init-param>
		<init-param>
			<param-name>sessionIdField</param-name>
			<param-value/>
		</init-param>
		<load-on-startup>3</load-on-startup>
	</servlet>
	<!-- 直联券商通信-->
	<servlet>
		<servlet-name>ZLSecuChannel</servlet-name>
		<servlet-class>core.communication.access.zlsecu.PBankTCPIPServiceServlet</servlet-class>
		<init-param>
			<param-name>factoryName</param-name>
			<param-value>SFOnline</param-value>
		</init-param>
		<init-param>
			<param-name>rootContextName</param-name>
			<param-value>rootCtx</param-value>
		</init-param>
		<init-param>
			<param-name>servletContextFile</param-name>
			<param-value>WEB-INF/channels/ZLSecuChannel.xml</param-value>
		</init-param>
		<init-param>
			<param-name>sessionIdField</param-name>
			<param-value/>
		</init-param>
		<load-on-startup>4</load-on-startup>
	</servlet>
	<!-- 深证通通信-->
	<servlet>
		<servlet-name>SZTSecuChannel</servlet-name>
		<servlet-class>core.communication.access.stzsecu.SZTTCPIPServiceServlet</servlet-class>
		<init-param>
			<param-name>factoryName</param-name>
			<param-value>SFOnline</param-value>
		</init-param>
		<init-param>
			<param-name>rootContextName</param-name>
			<param-value>rootCtx</param-value>
		</init-param>
		<init-param>
			<param-name>servletContextFile</param-name>
			<param-value>WEB-INF/channels/SZTSecuChannel.xml</param-value>
		</init-param>
		<init-param>
			<param-name>poolSize</param-name>
			<param-value>5</param-value>
		</init-param>
		<init-param>
			<param-name>timeout</param-name>
			<param-value>40000</param-value>
		</init-param>
		<init-param>
			<param-name>isStartPolling</param-name>
			<param-value>off</param-value>
		</init-param>
		<init-param>
			<param-name>sessionIdField</param-name>
			<param-value/>
		</init-param>
		<load-on-startup>5</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ESBChannel</servlet-name>
		<url-pattern>/ESBChannel/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CobankChannel</servlet-name>
		<url-pattern>/CobankChannel/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ZLSecuChannel</servlet-name>
		<url-pattern>/ZLSecuChannel/*</url-pattern>
	</servlet-mapping>	
	<!--<servlet-mapping>
		<servlet-name>SZTSecuChannel</servlet-name>
		<url-pattern>/SZTSecuChannel/*</url-pattern>
	</servlet-mapping>
	--><servlet-mapping>
		<servlet-name>SFOnlineServlet</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	
	<!-- ISC MQ通信-->
	<listener>
		<listener-class>com.isc.util.StartupListener</listener-class>
  	</listener>
	<welcome-file-list>
		<welcome-file>index.do</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>