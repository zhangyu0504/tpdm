			investData.setInvType( invType );
			investData.setInvName( invName );
			investData.setIdType( hostIdType );
			investData.setInvIdCode( invIdCode );

			if( SFConst.INV_TYPE_RETAIL.equals( invType ) ) {
				investData.setLegalName( legalName );
