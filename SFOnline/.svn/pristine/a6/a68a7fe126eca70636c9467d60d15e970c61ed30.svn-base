			buffer.append("T1.CHLTRANSEQID = T2.CHLTRANSEQID,");
		}
		if (UMIdChanged) {
			buffer.append("T1.UMID = T2.UMID,");
		}
		if (ybStatusChanged) {
			buffer.append("T1.YBSTATUS = T2.YBSTATUS");
		}
		if (buffer.substring(buffer.length() - 1).equals(",")) {
			buffer = buffer.deleteCharAt(buffer.length() - 1);
		}
		buffer.append(" WHEN NOT MATCHED THEN");
		buffer.append(" insert(TXDATE,INITSIDE,USERID,TXSEQID,REVTXSEQID,SECSEQID,SUBTXSEQID,INVTYPE,INVNAME,IDTYPE,INVIDCODE,OLDINVNAME,OLDIDTYPE,");
		buffer.append("OLDINVIDCODE,SECACCT,SECACCTSEQ,SECCOMPCODE,CAPACCT,ACCTID,SAVACCT,OPENDEPID,OPENBRANCHID,OLDACCTID,CURCODE,DCFLAG,TXAMOUNT,");
		buffer.append("ACCTBAL,ABSTRACT,ABSTRACTSTR,JOURFLAG,TXCODE,BUSITYPE,TXTIME,DEPID,BRANCHID,UNITTELLERID,CASHREMITFLAG,ACCTDEALID,ACCTHOSTSEQID,");
		buffer.append("PRESEQID,ACCTDEALDATE,BANKSEQID,PRODUCTTYPE,COLFLAG,ABORTFLAG,ABNDEALTIMES,ABNDEALTXTIME,MEMO,RESPCODE,RESPMSG,TRANSEQID,BUSISEQID,");
		buffer.append("CHLSEQID,CHLTRANSEQID,UMID,YBSTATUS)");
		buffer.append(" values(T2.TXDATE,T2.INITSIDE,T2.USERID,T2.TXSEQID,T2.REVTXSEQID,T2.SECSEQID,T2.SUBTXSEQID,T2.INVTYPE,T2.INVNAME,T2.IDTYPE,T2.INVIDCODE,");
		buffer.append("T2.OLDINVNAME,T2.OLDIDTYPE,T2.OLDINVIDCODE,T2.SECACCT,T2.SECACCTSEQ,T2.SECCOMPCODE,T2.CAPACCT,T2.ACCTID,T2.SAVACCT,T2.OPENDEPID,T2.OPENBRANCHID,");
		buffer.append("T2.OLDACCTID,T2.CURCODE,T2.DCFLAG,T2.TXAMOUNT,T2.ACCTBAL,T2.ABSTRACT,T2.ABSTRACTSTR,T2.JOURFLAG,T2.TXCODE,T2.BUSITYPE,T2.TXTIME,T2.DEPID,T2.BRANCHID,");
		buffer.append("T2.UNITTELLERID,T2.CASHREMITFLAG,T2.ACCTDEALID,T2.ACCTHOSTSEQID,T2.PRESEQID,T2.ACCTDEALDATE,T2.BANKSEQID,T2.PRODUCTTYPE,T2.COLFLAG,T2.ABORTFLAG,");
		buffer.append("T2.ABNDEALTIMES,T2.ABNDEALTXTIME,T2.MEMO,T2.RESPCODE,T2.RESPMSG,T2.TRANSEQID,T2.BUSISEQID,T2.CHLSEQID,T2.CHLTRANSEQID,T2.UMID,T2.YBSTATUS)");

		return new SQLStruct(buffer.toString(), valueObj);

	}
