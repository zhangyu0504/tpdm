		buffer.append(" ? as INVNAME, ? as IDTYPE, ? as INVIDCODE, ? as OLDINVNAME, ? as OLDIDTYPE, ? as OLDINVIDCODE, ? as SECACCT, ? as SECACCTSEQ, ? as SECCOMPCODE,");
